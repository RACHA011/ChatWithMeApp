{
  "expo": {
    "name": "CWM",
    "slug": "CWM",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/playstore.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.racha033.CWM"
    },
    "web": {
      "favicon": "./assets/playstore.png"
    },
    "extra": {
      "eas": {
        "projectId": "1fffc7cb-0887-4dc1-866e-45d8013d6f9e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1fffc7cb-0887-4dc1-866e-45d8013d6f9e"
    },
    "owner": "racha033"
  }
}
